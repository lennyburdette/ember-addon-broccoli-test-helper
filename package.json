{
  "name": "ember-addon-broccoli-test-helper",
  "version": "0.0.0",
  "description": "Helpers for testing the broccoli build aspects of Ember Addons",
  "keywords": [
    "ember-addon"
  ],
  "author": "Square, Inc.",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "!dist/test"
  ],
  "repository": "https://github.com/lennyburdette/ember-addon-broccoli-test-helper",
  "scripts": {
    "build": "tsc",
