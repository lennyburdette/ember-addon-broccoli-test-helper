    "test:debug": "tsc && mocha debug dist/test/"
  },
  "dependencies": {
    "broccoli-test-helper": "^1.2.0",
    "ember-cli-babel": "^6.6.0",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/lodash": "^4.14.109",
    "@types/mocha": "^5.2.2",
    "@types/node": "^10.3.4",
    "chai": "^4.1.2",
    "ember-cli": "~3.1.2",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.2.0",
    "loader.js": "^4.2.3",
    "mocha": "^5.2.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  },
  "engines": {
    "node": "6.* || >= 7.*"
  },
  "ember-addon": {
    "main": "ember-addon-main.js"
  }
